{
  "name": "backend",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node ./src/server.ts",
    "lint": "./node_modules/.bin/eslint \"**/*.ts\" \"**/*.js\"",
    "lint:fix": "./node_modules/.bin/eslint \"**/*.ts\" \"**/*.js\" --fix",
    "prettier": "./node_modules/.bin/prettier **/*.ts **/*.js **/*.html **/*.css --ignore-path=node_modules --ignore-path=**/build --ignore-path=**/dist --check",
    "prettier:fix": "./node_modules/.bin/prettier **/*.ts **/*.js **/*.html **/*.css --ignore-path=node_modules --ignore-path=**/build --ignore-path=**/dist --write",
    "build": "tsc --project tsconfig.json",
    "clean": "rimraf dist && rimraf coverage",
    "prebuild": "npm run clean",
    "start": "node ./build/server.js",
    "watch": "nodemon -e ts -w ./src -x npm run watch:serve",
    "watch:serve": "ts-node src/index.ts"
  },
  "_devDependencies": {
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/config": "^0.0.34",
    "@types/graphql": "^14.2.0",
    "@types/mongoose": "^5.5.3",
    "@types/node": "^12.0.4",
    "husky": "^2.3.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  },
  "_dependencies": {
    "@accounts/database-manager": "^0.13.0",
    "@accounts/graphql-api": "^0.13.0",
    "@accounts/mongo": "^0.13.0",
    "@accounts/password": "^0.13.0",
    "@accounts/server": "^0.13.0",
    "@graphql-modules/core": "^0.7.5",
    "apollo-server": "^2.6.1",
    "apollo-server-express": "^2.6.1",
    "config": "^3.1.0",
    "graphql": "^14.3.1",
    "graphql-toolkit": "^0.2.14",
    "mongoose": "^5.5.12",
    "plaid": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.4",
    "typegoose": "^5.6.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "express": "^4.17.1",
    "prettier": "^1.18.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  }
}
