{
  "name": "frontend",
  "version": "0.0.1",
  "scripts": {
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "prettier": "prettier **/*.ts **/*.js **/*.html **/*.css --ignore-path node_modules --ignore-path build --ignore-path dist --check",
    "prettier:write": "prettier **/*.ts **/*.js **/*.html **/*.css --ignore-path node_modules --ignore-path build --ignore-path dist --write",
    "start": "npx http-server --cors -o",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "http-server": "^0.11.1"
  }
}
